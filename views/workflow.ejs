<g
  id="<%= process.id %>"
  transform="translate(100,100)"
  xmlns="http://www.w3.org/2000/svg"
>
  <circle r="60" xmlns="http://www.w3.org/2000/svg"></circle>
  <text
    y="5"
    text-anchor="middle"
    fill="black"
    class="process-label"
    xmlns="http://www.w3.org/2000/svg"
  >
    <%= process.name %>
  </text>
</g>
